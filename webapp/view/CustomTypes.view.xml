<mvc:View controllerName="private.sandbox.controller.CustomTypes"
          xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
          xmlns="sap.m">

    <Page title="{i18n>titleCustomTypes}" showNavButton="true" navButtonPress="onNavBack">
        <content>
            <Input id="emailInput" value="{path:'/customTypes/input', model:'util', type:'private.sandbox.util.types.Email'}"/>
            <Button text="Validieren" press="onValidate"/>
        </content>
    </Page>
</mvc:View>